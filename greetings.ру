# Тестовый ввод переменных
username = "alex"
title = "home_work"
content = "download_new_code_program"
status = "work"
created_date = "10.11.2024"  # Для отображения только дня и месяца use - [:-5]
issue_date = "10.12.2024"  # Для отображения только дня и месяца use - [:-5]
# Программа
print("Имя пользователя:", username)
print("Заголовок заметки:", title)
print("Описание заметки:", content)
print("Статус заметки:", status)
print("День-Месяц-Год:", created_date)  # (дата создания заметки)
print("День-Месяц-Год:", issue_date)  # (дата истечения заметки - Дедлайн)
